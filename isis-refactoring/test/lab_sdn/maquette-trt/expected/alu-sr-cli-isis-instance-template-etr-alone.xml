<devices xmlns="http://tail-f.com/ns/ncs">
  <device>
    <name>ETR01-SDN-01</name>
    <config>
      <router xmlns="http://tail-f.com/ned/alu-sr">
        <router-name>Base</router-name>
        <isis-list>
          <id>1</id>
          <advertise-passive-only/>
          <advertise-router-capability>area</advertise-router-capability>
          <area-id>
            <id>49.0011</id>
          </area-id>
          <disable-ldp-sync/>
          <ipv6-routing>mt</ipv6-routing>
          <level>
            <id>1</id>
            <wide-metrics-only/>
          </level>
          <level>
            <id>2</id>
            <wide-metrics-only/>
          </level>
          <lsp-lifetime>65535</lsp-lifetime>
          <overload-on-boot>
            <timeout>360</timeout>
          </overload-on-boot>
          <segment-routing>
            <prefix-sid-range>
              <global/>
            </prefix-sid-range>
            <shutdown>false</shutdown>
          </segment-routing>
          <timers>
            <spf-wait>
              <wait>2000</wait>
              <spf-initial-wait>50</spf-initial-wait>
              <spf-second-wait>100</spf-second-wait>
            </spf-wait>
          </timers>
          <shutdown>false</shutdown>
          <loopfree-alternates>
            <ti-lfa>
              <node-protect/>
            </ti-lfa>
          </loopfree-alternates>
        </isis-list>
      </router>
    </config>
  </device>
</devices>
<rfs xmlns="http://bouyguestelecom.fr/rfs">
  <isis xmlns="http://bouyguestelecom.fr/isis">
    <device>ETR01-SDN-01</device>
    <instance>
      <instance-id>1</instance-id>
      <area-id>49.0011</area-id>
      <loopback0>10.209.220.236</loopback0>
      <sr>
        <lower-bound>18432</lower-bound>
        <upper-bound>118432</upper-bound>
      </sr>
      <disable-sync-ldp/>
      <dist-link-state/>
    </instance>
  </isis>
</rfs>
